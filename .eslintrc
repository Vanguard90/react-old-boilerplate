{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/jsx-indent": 0,
    "react/jsx-tag-spacing": 0,
    "padded-blocks": 0,
    "linebreak-style": 0,
    "indent": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0,
    "react/destructuring-assignment": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "prefer-destructuring": 0,
    "react/jsx-no-bind": 0,
    "no-param-reassign": 0,
    "react/sort-comp": 0,
    "consistent-return": 0,
    "no-plusplus": 0
  }
}
